<html lang='en'>
  <head>
    <meta charset='utf-8' />

    <link href='calendar_main.css' rel='stylesheet' />
    <link href='calendar_daygrid.css' rel='stylesheet' />

    <script src='calendar_main.js'></script>
    <script src='calendar_daygrid.js'></script>

    <script>

      document.addEventListener('DOMContentLoaded', function() {
        var calendarEl = document.getElementById('calendar');

        var calendar = new FullCalendar.Calendar(calendarEl, {
          plugins: [ 'dayGrid' ]
        });

        calendar.render();
      });

    </script>
  </head>
  <body>
    <h1>My Schedule</h1>
    <div id='calendar'></div>

  </body>
</html>
