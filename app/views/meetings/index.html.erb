<p id="notice"><%= notice %></p>

<h1>Meetings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Tutor</th>
      <th>Student</th>
      <th>Location</th>
      <th>Startdatetime</th>
      <th>Enddatetime</th>
      <th>Student accepted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meetings.each do |meeting| %>
      <tr>
        <td><%= meeting.title %></td>
        <td><%= meeting.description %></td>
        <td><%= meeting.tutor_id %></td>
        <td><%= meeting.student_id %></td>
        <td><%= meeting.location %></td>
        <td><%= meeting.startDateTime %></td>
        <td><%= meeting.endDateTime %></td>
        <td><%= meeting.student_accepted %></td>
        <td><%= link_to 'Show', meeting %></td>
        <td><%= link_to 'Edit', edit_meeting_path(meeting) %></td>
        <td><%= link_to 'Destroy', meeting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meeting', new_meeting_path %>
